    "discord.js": "^13.1.0",
    "dotenv": "^8.6.0",
    "element-plus": "^1.1.0-beta.8",
    "fs": "^0.0.1-security",
    "fs-extra": "^7.0.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.0-0",
    "vue3-cookies": "^1.0.1",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
